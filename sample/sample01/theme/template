(:html (:lang "ja")
       ((:head  ((:meta (:charset "UTF-8"))
                 (:meta (:name "viewport" :content "width = device-width, initial-scale = 1, user-scalable = yes"))
                 (:link (:rel "stylesheet" :type "text/css" :href "style.css")) 
                 (:link (:rel "alternate" :type "application/atom+xml" :title "Atom 1.0" :href "feed.xml"))
                 (:title (get-prop "site-name" "project") "-" (get-prop "title" "this"))))
       	(:body  ((:header (:id "main-header")
                  ((:h1 (:a (:href "index.htm") (get-prop "site-name" "project")))))
                 (:main (:header (:id "article-header")
                         ((:div (:id "date") (get-prop "date" "this"))
                          (:h2 (:id "title") (get-prop "title" "this"))))
                        (:div (:id "article-main")
                              ((get-prop "text" "this" "md")))
                        (:footer (:id "article-footer")
                                 ("PREV: " (anchor (string-add (get-prop "prev" "this") ".html") (get-prop "title" (get-prop "prev")))
                                  "NEXT: " (anchor (string-add (get-prop "next" "this") ".html") (get-prop "title" (get-prop "next"))))))
                 (:footer (:id "main-footer")
	                  ((:div (:id "copyright") "&copy;" (get-prop "pubyear" "project") (get-prop "author" "project"))))))))
